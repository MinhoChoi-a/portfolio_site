body
  section.intro
    img.image(src='img/back.jpg' alt='')
    .inner
      #content.animate__animated.animate__bounceInUp
        .title
          p Hello World!
        button View my works
  
  .mainNav
     nav(class="navbar navbar-expand-md")
        div(class="container-fluid")
          button(class="navbar-brand") MINHO Studio
          
          button(class="navbar-toggler" data-toggle="collapse" data-target="#navbarResponsive" type="button" aria-controls="navbarResponsive" aria-expanded="true" aria-label="Toggle navigation")
            span(class="material-icons") menu
                
          div(class="navbar-collapse collapse" id="navbarResponsive")
            ul(class="nav navbar-nav mr-auto mt-2 mt-lg-0 text-uppercase")
              li(class="nav-item" role="presentation")
                button.nav-link_work(data-toggle="collapse" data-target="#navbarResponsive") WORKS
